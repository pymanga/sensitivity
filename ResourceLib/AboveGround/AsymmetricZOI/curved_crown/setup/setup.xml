<MangaProject>
    <random_seed>%seed%</random_seed>
    <resources>
        <aboveground>
            <type> AsymmetricZOI </type>
            <domain>
                <x_1> 0 </x_1>
                <y_1> 0 </y_1>
                <x_2> 5 </x_2>
                <y_2> 5 </y_2>
                <x_resolution> 20 </x_resolution>
                <y_resolution> 20 </y_resolution>
            </domain>
            <curved_crown>%curved%</curved_crown>    
        </aboveground>
        <belowground>
            <type> Default </type>
        </belowground>
    </resources>
    <population>
        <group>
            <name> Initial </name>
            <species> Avicennia </species>
            <vegetation_model_type> Bettina </vegetation_model_type>
            <mortality>NoGrowth</mortality>
            <distribution>
                <type> Random </type>
                <domain>
                    <x_1> 0 </x_1>
                    <y_1> 0 </y_1>
                    <x_2> 5 </x_2>
                    <y_2> 5 </y_2>
                </domain>
                <n_recruitment_per_step> 0 </n_recruitment_per_step>
                <n_individuals> 2 </n_individuals>
            </distribution>
        </group>
    </population>
    <time_loop>
        <type> Simple </type>
        <t_start> 0 </t_start>
        <t_end> 3600*24*365.25 * 100 </t_end>
        <delta_t> 3600*24*365.25/12 </delta_t>
        <terminal_print>years</terminal_print>
    </time_loop>
    <visualization>
        <type> NONE </type>
    </visualization>
    <output>
        <type> OneFile </type>
        <allow_previous_output>True</allow_previous_output>
        <output_each_nth_timestep>[1]</output_each_nth_timestep>
        <output_dir> %out_dir%</output_dir>
        <geometry_output> r_stem </geometry_output>
        <geometry_output> h_stem </geometry_output>
        <geometry_output> r_crown </geometry_output>
        <geometry_output> r_root </geometry_output>
        <growth_output> growth </growth_output>
        <growth_output> ag_factor </growth_output>
        <growth_output> bg_factor </growth_output>
    </output>
</MangaProject>

